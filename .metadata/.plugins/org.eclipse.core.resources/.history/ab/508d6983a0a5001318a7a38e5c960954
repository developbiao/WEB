package learn.base_01;

public class StringDemo_01 {
	
	public static void main(String[] args){
		
		String s1 = "hello";
		String s2 = "world";
		String s3 = "hello";
		System.out.println(s1 == s2); //true
		s1 = new String("hello");
		s2 = new String("hello");
		System.out.println(s1 == s2);  //false;
		System.out.println("is ok !");
	}
}
